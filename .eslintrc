{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "strict": 1,
    "eqeqeq": 2,
    "curly": [2, "multi-line"],
    "quotes": [2, "single"],
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 2
  }
}